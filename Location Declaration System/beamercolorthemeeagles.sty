\mode<presentation>

%define some nice colors 
	\definecolor{eagleblue}{RGB}{25,74,188}
	\definecolor{eaglegold}{RGB}{255,189,77}
	\definecolor{darkgreen}{RGB}{0,122,64}
	\definecolor{lightgreen}{RGB}{89,184,130}
	\definecolor{eaglebrown}{RGB}{104,50,16}



\setbeamercolor{structure}{fg=eagleblue}

\setbeamercolor{palette primary}{fg=black,bg=eaglegold}
\setbeamercolor{palette secondary}{fg=lightgreen,bg=eagleblue!75!black}
\setbeamercolor{palette tertiary}{fg=black,bg=eaglegold!90!black}
\setbeamercolor{palette quaternary}{fg=black,bg=darkgreen}

%\setbeamercolor{titlelike}{parent=palette quaternary}
\setbeamercolor{titlelike}{parent=palette primary}

\setbeamercolor{block title}{fg=eaglegold,bg=lightgreen}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
%\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}
\setbeamercolor{block title example}{fg=eagleblue,bg=eaglegold}

%\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.fg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
%\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,fg=block title example.bg,bg=block title example.fg}

%\setbeamercolor{sidebar}{bg=eaglegold!70}
\setbeamercolor{sidebar}{fg=white,bg=eagleblue}
  
